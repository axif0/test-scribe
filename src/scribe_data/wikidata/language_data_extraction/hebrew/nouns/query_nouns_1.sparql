  ?pluralFeminine
  ?singularConstructMasculine
  ?pluralIndefinite
  ?singularMasculine
  ?dualFeminine
  ?singularFeminineMasculine
  ?feminine
  ?dualConstructFeminine
  ?singularIndefinite
  ?singularConstruct
  ?singularNominative
  ?pluralDefinite
  ?singular
  ?singularConstructFeminine
