   (REPLACE(STR(?lexeme), "http://www.wikidata.org/entity/", "") AS ?lexemeID)
    ?nouns
    ?pluralMasculine
  ?singularConstructFeminineMasculine
  ?singularFeminine
  ?feminineMasculine
  ?pluralConstructFeminineMasculine
  ?pluralConstructMasculine
  ?pluralConstruct
  ?nominativePlural
  ?pluralConstructFeminine
  ?dualMasculine
  ?dualConstructMasculine
  ?masculine
  ?singularPluralConstructMasculine
  ?pluralFeminineMasculine
