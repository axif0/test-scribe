
# tool: scribe-data
# All finnish (Q1412) nouns (Q1084) and their forms.
# Enter this query at https://query.wikidata.org/.

SELECT
   (REPLACE(STR(?lexeme), "http://www.wikidata.org/entity/", "") AS ?lexemeID)
    ?nouns
    ?singularAbessive
  ?singularNominative
  ?singularPartitive
  ?pluralEssive
  ?singularGenitive
  ?pluralInessive
  ?accusativePlural
  ?singularInessive
  ?singularEssive
  ?singularAccusative
  ?pluralAblative
  ?pluralElative
  ?pluralAdessive
  ?pluralAllative
  ?comitative
  ?singularAdessive
  ?partitive
  ?singularAblative
  ?pluralTranslative
  ?singularElative
  ?singularAllative
  ?genitivePlural
  ?pluralIllative
  ?singularTranslative
  ?pluralComitative
  ?pluralAbessive
  ?pluralPartitive
  ?singularIllative
  WHERE {
    ?lexeme dct:language wd:Q1412 ;
        wikibase:lexicalCategory wd:Q1084 ;
        wikibase:lemma ?nouns .
        FILTER(lang(?nouns) = "fi")
    
        OPTIONAL {
            ?lexeme ontolex:lexicalForm ?singularAbessiveForm .
            ?singularAbessiveForm ontolex:representation ?singularAbessive ;
                wikibase:grammaticalFeature wd:Q110786, wd:Q319822 .
        }
