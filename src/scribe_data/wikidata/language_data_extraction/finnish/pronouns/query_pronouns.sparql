# tool: scribe-data
# All finnish (Q1412) pronouns (Q36224) and their forms.
# Enter this query at https://query.wikidata.org/.

SELECT
  (REPLACE(STR(?lexeme), "http://www.wikidata.org/entity/", "") AS ?lexemeID)
  ?pronouns
    ?singularInessive
  ?pluralEssive
  ?partitive
  ?singularAblative
  ?pluralPartitive
  ?accusative
  ?singularAbessive
  ?nominative
  ?singularGenitive
  ?illative
  ?genitivePlural
  ?singularAdessive
  ?translative
  ?singularElative
  ?pluralIllative
  ?allative
  ?pluralTranslative
  ?singularEssive
  ?pluralElative
  ?pluralAllative
  ?pluralComitative
  ?inessive
  ?singularPartitive
  ?ablative
  ?pluralInessive
  ?pluralAblative
  ?singularNominative
  ?singularIllative
  ?genitive
  ?adessive
  ?singularTranslative
  ?pluralAbessive
  ?nominativePlural
  ?elative
  ?pluralAdessive
  ?singularAllative
  ?essive

WHERE {
  ?lexeme dct:language wd:Q1412 ;
  wikibase:lexicalCategory wd:Q36224 ;
  wikibase:lemma ?pronouns .
  FILTER(lang(?pronouns) = "fi")
    
  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?singularInessiveForm .
    ?singularInessiveForm ontolex:representation ?singularInessive ;
      wikibase:grammaticalFeature wd:Q110786, wd:Q282031 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?pluralEssiveForm .
