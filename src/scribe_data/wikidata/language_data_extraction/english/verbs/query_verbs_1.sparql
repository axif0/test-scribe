# tool: scribe-data
# All English (Q1860) verbs (Q24905) and their forms.
# Enter this query at https://query.wikidata.org/.

SELECT
  (REPLACE(STR(?lexeme), "http://www.wikidata.org/entity/", "") AS ?lexemeID)
  ?verbs
  ?infinitive
  ?pastParticiple
  ?pastParticipleSimplePast
  ?past
  ?singularFirstPersonSimplePresent
  ?singularSimplePresentSecondPerson
  ?singularSimplePastSecondPerson
  ?singularThirdPersonPresentIndicative
  ?simplePresent
  ?singularPreteriteThirdPersonIndicative
  ?contraction
  ?presentParticiple
  ?simplePastPlural
  ?pluralPreteriteIndicative
  ?singularFirstPersonPreteriteIndicative
  ?secondPersonPresentIndicative
