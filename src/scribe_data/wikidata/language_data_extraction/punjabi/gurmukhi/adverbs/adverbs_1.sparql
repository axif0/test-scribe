# Enter this query at https://query.wikidata.org/.

SELECT
  (REPLACE(STR(?lexeme), "http://www.wikidata.org/entity/", "") AS ?lexemeID)
  ?adverbs
  ?comparativeOblique
  ?ablativeComparative
  ?femininePluralOblique
  ?feminineObliqueSingular
  ?positiveOblique
  ?pluralOblique
  ?positiveAblative
  ?obliqueSingular
  ?feminineDirectSingular
  ?positiveDirect
  ?directComparative
  ?ablativeSingular
  ?masculineDirectSingular
  ?masculineDirectPlural
  ?masculineObliqueSingular
  ?masculinePluralOblique
  ?locativeSingular
  ?directSingular
  ?locativePlural
  ?feminineDirectPlural
  ?directPlural
  ?obliqueComparative
  ?singularObliqueMasculine
  ?obliquePositive
  ?singularOblique
  ?singularLocative
  ?pluralLocative
  ?comparativeAblative
  ?singularAblative
  ?ablativePositive
  ?comparativeDirect
  ?singularObliqueFeminine
