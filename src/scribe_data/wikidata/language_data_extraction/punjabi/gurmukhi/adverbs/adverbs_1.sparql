  FILTER(lang(?adverbs) = "pa")
    
  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?comparativeObliqueForm .
    ?comparativeObliqueForm ontolex:representation ?comparativeOblique ;
      wikibase:grammaticalFeature wd:Q14169499, wd:Q1233197 .
