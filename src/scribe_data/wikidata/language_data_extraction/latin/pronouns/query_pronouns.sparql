  ?feminineNominativeSingular
  ?neuterAblativeSingular
  ?masculinePluralGenitive
  ?genitiveSingular
  ?femininePluralGenitive
  ?neuterPluralAccusative
  ?masculineAblativeSingular
  ?ablativePlural
  ?dativeSingular
  ?neuterNominativeSingular
  ?neuterAccusativeSingular
   ?lastModified

WHERE {
  ?lexeme dct:language wd:Q397 ;
      wikibase:lexicalCategory wd:Q36224 ;
      wikibase:lemma ?pronouns ;
      schema:dateModified ?lastModified .
    
  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?feminineAblativeSingularForm .
    ?feminineAblativeSingularForm ontolex:representation ?feminineAblativeSingular ;
      wikibase:grammaticalFeature wd:Q1775415, wd:Q156986, wd:Q110786 .
  }

