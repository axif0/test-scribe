# tool: scribe-data
# All Latin (Q397) pronouns (Q36224) and their forms.
# Enter this query at https://query.wikidata.org/.

SELECT
  (REPLACE(STR(?lexeme), "http://www.wikidata.org/entity/", "") AS ?lexemeID)
  ?pronouns
  ?feminineAblativeSingular
  ?masculineAccusativeSingular
  ?masculinePluralAccusative
  ?neuterPluralGenitive
  ?femininePluralAccusative
  ?masculinePluralNominative
  ?masculineNominativeSingular
  ?pluralDative
  ?feminineAccusativeSingular
  ?neuterPluralNominative
