# tool: scribe-data
# All latin (Q397) pronouns (Q36224) and their forms.
# Enter this query at https://query.wikidata.org/.

SELECT
   (REPLACE(STR(?lexeme), "http://www.wikidata.org/entity/", "") AS ?lexemeID)
    ?pronouns
    ?accusativePluralMasculine
  ?genitivePluralNeuter
  ?singularAblativeMasculine
  ?singularAccusativeMasculine
  ?genitivePluralFeminine
  ?accusativePluralNeuter
  ?singularAblativeNeuter
  ?singularAccusativeNeuter
  ?accusativePluralFeminine
  ?singularNominativeMasculine
  ?singularAblativeFeminine
  ?singularGenitive
  ?singularAccusativeFeminine
  ?nominativePluralMasculine
  ?singularDative
  ?singularNominativeNeuter
  ?singularNominativeFeminine
  ?dativePlural
  ?nominativePluralNeuter
  ?genitivePluralMasculine
  ?nominativePluralFeminine
  ?pluralAblative
  WHERE {
    ?lexeme dct:language wd:Q397 ;
        wikibase:lexicalCategory wd:Q36224 ;
        wikibase:lemma ?pronouns .
        FILTER(lang(?pronouns) = "la")
    
        OPTIONAL {
            ?lexeme ontolex:lexicalForm ?accusativePluralMasculineForm .
            ?accusativePluralMasculineForm ontolex:representation ?accusativePluralMasculine ;
                wikibase:grammaticalFeature wd:Q146078, wd:Q146786, wd:Q499327 .
        }

        OPTIONAL {
            ?lexeme ontolex:lexicalForm ?genitivePluralNeuterForm .
            ?genitivePluralNeuterForm ontolex:representation ?genitivePluralNeuter ;
                wikibase:grammaticalFeature wd:Q146233, wd:Q146786, wd:Q1775461 .
        }

        OPTIONAL {
            ?lexeme ontolex:lexicalForm ?singularAblativeMasculineForm .
            ?singularAblativeMasculineForm ontolex:representation ?singularAblativeMasculine ;
                wikibase:grammaticalFeature wd:Q110786, wd:Q156986, wd:Q499327 .
