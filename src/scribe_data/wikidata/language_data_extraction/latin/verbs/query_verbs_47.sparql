# tool: scribe-data
# All Latin (Q397) verbs (Q24905) and the given forms.
# Enter this query at https://query.wikidata.org/.

SELECT
  (REPLACE(STR(?lexeme), "http://www.wikidata.org/entity/", "") AS ?lexemeID)
  ?lastModified
  ?verb
  ?indicativeActivePresentFirstPersonSingular
  ?indicativeActiveFutureThirdPersonSingular
  ?indicativeActivePastImperfectSecondPersonSingular
  ?indicativeActivePastImperfectThirdPersonPlural
  ?indicativeActivePluperfectSecondPersonPlural
  ?indicativeActiveFuturePerfectThirdPersonSingular

WHERE {
  ?lexeme dct:language wd:Q397 ;
      wikibase:lexicalCategory wd:Q24905 ;
      wikibase:lemma ?verb ;
      schema:dateModified ?lastModified .
    
  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?indicativeActivePresentFirstPersonSingularForm .
    ?indicativeActivePresentFirstPersonSingularForm ontolex:representation ?indicativeActivePresentFirstPersonSingular ;
      wikibase:grammaticalFeature wd:Q682111, wd:Q1317831, wd:Q192613, wd:Q21714344, wd:Q110786 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?indicativeActiveFutureThirdPersonSingularForm .
    ?indicativeActiveFutureThirdPersonSingularForm ontolex:representation ?indicativeActiveFutureThirdPersonSingular ;
      wikibase:grammaticalFeature wd:Q682111, wd:Q1317831, wd:Q501405, wd:Q51929074, wd:Q110786 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?indicativeActivePastImperfectSecondPersonSingularForm .
    ?indicativeActivePastImperfectSecondPersonSingularForm ontolex:representation ?indicativeActivePastImperfectSecondPersonSingular ;
      wikibase:grammaticalFeature wd:Q682111, wd:Q1317831, wd:Q12547192, wd:Q51929049, wd:Q110786 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?indicativeActivePastImperfectThirdPersonPluralForm .
    ?indicativeActivePastImperfectThirdPersonPluralForm ontolex:representation ?indicativeActivePastImperfectThirdPersonPlural ;
      wikibase:grammaticalFeature wd:Q682111, wd:Q1317831, wd:Q12547192, wd:Q51929074, wd:Q146786 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?indicativeActivePluperfectSecondPersonPluralForm .
    ?indicativeActivePluperfectSecondPersonPluralForm ontolex:representation ?indicativeActivePluperfectSecondPersonPlural ;
      wikibase:grammaticalFeature wd:Q682111, wd:Q1317831, wd:Q623742, wd:Q51929049, wd:Q146786 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?indicativeActiveFuturePerfectThirdPersonSingularForm .
    ?indicativeActiveFuturePerfectThirdPersonSingularForm ontolex:representation ?indicativeActiveFuturePerfectThirdPersonSingular ;
      wikibase:grammaticalFeature wd:Q682111, wd:Q1317831, wd:Q501405, wd:Q625420, wd:Q51929074, wd:Q110786 .
  }
}
