  ?singularMasculine
  ?pluralMasculine
  ?dativeAccusativeFeminineMasculine
  ?accusativeFeminine
  ?singularFeminine
  ?pluralFeminine
  ?dativeMasculine
  ?singularNominativePluralFeminineVocativeMasculine
  ?feminineMasculine
  ?singularNeuter
  ?dativeFeminine
  ?pluralNeuter
   ?lastModified

WHERE {
  ?lexeme dct:language wd:Q1321 ;
      wikibase:lexicalCategory wd:Q468801 ;
      wikibase:lemma ?personal_pronouns ;
      schema:dateModified ?lastModified .
    
