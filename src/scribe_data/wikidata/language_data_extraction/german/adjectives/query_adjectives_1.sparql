# tool: scribe-data
# All german (Q188) adjectives (Q34698) and their forms.
# Enter this query at https://query.wikidata.org/.

SELECT
   (REPLACE(STR(?lexeme), "http://www.wikidata.org/entity/", "") AS ?lexemeID)
    ?adjectives
    ?comparative
  ?singularFeminine
  ?singularNeuter
  ?singularGenitive
  ?nominativeNeuter
  ?singularAccusative
  ?genitivePlural
  ?nominativePlural
  ?accusativePlural
  ?nominativeFeminine
  ?singularAccusativeMasculine
  ?singularMasculine
  ?singularNominative
  ?femininePositive
  ?singularDative
  ?superlative
  ?positive
  ?dativePlural
  ?nominativeMasculine
  WHERE {
    ?lexeme dct:language wd:Q188 ;
        wikibase:lexicalCategory wd:Q34698 ;
        wikibase:lemma ?adjectives .
        FILTER(lang(?adjectives) = "de")
    
        OPTIONAL {
            ?lexeme ontolex:lexicalForm ?comparativeForm .
            ?comparativeForm ontolex:representation ?comparative ;
                wikibase:grammaticalFeature wd:Q14169499 .
        }


        OPTIONAL {
            ?lexeme ontolex:lexicalForm ?singularFeminineForm .
            ?singularFeminineForm ontolex:representation ?singularFeminine ;
                wikibase:grammaticalFeature wd:Q110786, wd:Q1775415 .
        }


        OPTIONAL {
            ?lexeme ontolex:lexicalForm ?singularNeuterForm .
            ?singularNeuterForm ontolex:representation ?singularNeuter ;
                wikibase:grammaticalFeature wd:Q110786, wd:Q1775461 .
        }


        OPTIONAL {
            ?lexeme ontolex:lexicalForm ?singularGenitiveForm .
            ?singularGenitiveForm ontolex:representation ?singularGenitive ;
                wikibase:grammaticalFeature wd:Q110786, wd:Q146233 .
        }


        OPTIONAL {
            ?lexeme ontolex:lexicalForm ?nominativeNeuterForm .
            ?nominativeNeuterForm ontolex:representation ?nominativeNeuter ;
                wikibase:grammaticalFeature wd:Q131105, wd:Q1775461 .
        }


        OPTIONAL {
            ?lexeme ontolex:lexicalForm ?singularAccusativeForm .
            ?singularAccusativeForm ontolex:representation ?singularAccusative ;
                wikibase:grammaticalFeature wd:Q110786, wd:Q146078 .
        }


        OPTIONAL {
            ?lexeme ontolex:lexicalForm ?genitivePluralForm .
            ?genitivePluralForm ontolex:representation ?genitivePlural ;
                wikibase:grammaticalFeature wd:Q146233, wd:Q146786 .
        }


        OPTIONAL {
            ?lexeme ontolex:lexicalForm ?nominativePluralForm .
            ?nominativePluralForm ontolex:representation ?nominativePlural ;
                wikibase:grammaticalFeature wd:Q131105, wd:Q146786 .
        }


        OPTIONAL {
            ?lexeme ontolex:lexicalForm ?accusativePluralForm .
            ?accusativePluralForm ontolex:representation ?accusativePlural ;
                wikibase:grammaticalFeature wd:Q146078, wd:Q146786 .
        }


        OPTIONAL {
            ?lexeme ontolex:lexicalForm ?nominativeFeminineForm .
            ?nominativeFeminineForm ontolex:representation ?nominativeFeminine ;
                wikibase:grammaticalFeature wd:Q131105, wd:Q1775415 .
        }


        OPTIONAL {
            ?lexeme ontolex:lexicalForm ?singularAccusativeMasculineForm .
            ?singularAccusativeMasculineForm ontolex:representation ?singularAccusativeMasculine ;
                wikibase:grammaticalFeature wd:Q110786, wd:Q146078, wd:Q499327 .
        }


        OPTIONAL {
            ?lexeme ontolex:lexicalForm ?singularMasculineForm .
            ?singularMasculineForm ontolex:representation ?singularMasculine ;
                wikibase:grammaticalFeature wd:Q110786, wd:Q499327 .
        }


        OPTIONAL {
            ?lexeme ontolex:lexicalForm ?singularNominativeForm .
            ?singularNominativeForm ontolex:representation ?singularNominative ;
                wikibase:grammaticalFeature wd:Q110786, wd:Q131105 .
        }


        OPTIONAL {
            ?lexeme ontolex:lexicalForm ?femininePositiveForm .
            ?femininePositiveForm ontolex:representation ?femininePositive ;
                wikibase:grammaticalFeature wd:Q1775415, wd:Q3482678 .
        }


        OPTIONAL {
            ?lexeme ontolex:lexicalForm ?singularDativeForm .
            ?singularDativeForm ontolex:representation ?singularDative ;
                wikibase:grammaticalFeature wd:Q110786, wd:Q145599 .
        }


        OPTIONAL {
            ?lexeme ontolex:lexicalForm ?superlativeForm .
            ?superlativeForm ontolex:representation ?superlative ;
                wikibase:grammaticalFeature wd:Q1817208 .
        }


        OPTIONAL {
            ?lexeme ontolex:lexicalForm ?positiveForm .
            ?positiveForm ontolex:representation ?positive ;
                wikibase:grammaticalFeature wd:Q3482678 .
        }


        OPTIONAL {
            ?lexeme ontolex:lexicalForm ?dativePluralForm .
            ?dativePluralForm ontolex:representation ?dativePlural ;
                wikibase:grammaticalFeature wd:Q145599, wd:Q146786 .
        }


        OPTIONAL {
            ?lexeme ontolex:lexicalForm ?nominativeMasculineForm .
            ?nominativeMasculineForm ontolex:representation ?nominativeMasculine ;
                wikibase:grammaticalFeature wd:Q131105, wd:Q499327 .
        }

}