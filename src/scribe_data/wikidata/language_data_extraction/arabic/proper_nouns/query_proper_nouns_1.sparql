# tool: scribe-data
# All Arabic (Q13955) proper_nouns (Q147276) and their forms.
# Enter this query at https://query.wikidata.org/.

SELECT
  (REPLACE(STR(?lexeme), "http://www.wikidata.org/entity/", "") AS ?lexemeID)
  ?proper_nouns
  ?nominative
  ?singularPausalMasculineIndefinite
  ?singularGenitiveMasculine
  ?singularPausalFeminineIndefinite
  ?singularPausalMasculine
  ?singularGenitiveMasculineDefinite
  ?singularNominativeFeminine
