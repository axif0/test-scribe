# tool: scribe-data
# All Latvian (Q9078) verbs (Q24905) and their forms.
# Enter this query at https://query.wikidata.org/.

SELECT
  (REPLACE(STR(?lexeme), "http://www.wikidata.org/entity/", "") AS ?lexemeID)
  ?verbs
  ?indicativeSimplePresentFirstPersonPlural
  ?indicativeThirdPersonSimplePresentPlural
  ?infinitive
  ?indicativeSecondPersonSimplePresentSingular
  ?indicativeSecondPersonSimplePresentPlural
  ?indicativeThirdPersonSimplePresentSingular
  ?indicativeSimplePresentFirstPersonSingular
   ?lastModified

WHERE {
  ?lexeme dct:language wd:Q9078 ;
      wikibase:lexicalCategory wd:Q24905 ;
      wikibase:lemma ?verbs ;
      schema:dateModified ?lastModified .
    
  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?indicativeSimplePresentFirstPersonPluralForm .
    ?indicativeSimplePresentFirstPersonPluralForm ontolex:representation ?indicativeSimplePresentFirstPersonPlural ;
      wikibase:grammaticalFeature wd:Q682111, wd:Q3910936, wd:Q21714344, wd:Q146786 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?indicativeThirdPersonSimplePresentPluralForm .
    ?indicativeThirdPersonSimplePresentPluralForm ontolex:representation ?indicativeThirdPersonSimplePresentPlural ;
      wikibase:grammaticalFeature wd:Q682111, wd:Q51929074, wd:Q3910936, wd:Q146786 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?infinitiveForm .
    ?infinitiveForm ontolex:representation ?infinitive ;
      wikibase:grammaticalFeature wd:Q179230 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?indicativeSecondPersonSimplePresentSingularForm .
    ?indicativeSecondPersonSimplePresentSingularForm ontolex:representation ?indicativeSecondPersonSimplePresentSingular ;
      wikibase:grammaticalFeature wd:Q682111, wd:Q51929049, wd:Q3910936, wd:Q110786 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?indicativeSecondPersonSimplePresentPluralForm .
    ?indicativeSecondPersonSimplePresentPluralForm ontolex:representation ?indicativeSecondPersonSimplePresentPlural ;
      wikibase:grammaticalFeature wd:Q682111, wd:Q51929049, wd:Q3910936, wd:Q146786 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?indicativeThirdPersonSimplePresentSingularForm .
    ?indicativeThirdPersonSimplePresentSingularForm ontolex:representation ?indicativeThirdPersonSimplePresentSingular ;
      wikibase:grammaticalFeature wd:Q682111, wd:Q51929074, wd:Q3910936, wd:Q110786 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?indicativeSimplePresentFirstPersonSingularForm .
    ?indicativeSimplePresentFirstPersonSingularForm ontolex:representation ?indicativeSimplePresentFirstPersonSingular ;
      wikibase:grammaticalFeature wd:Q682111, wd:Q3910936, wd:Q21714344, wd:Q110786 .
  }
}
