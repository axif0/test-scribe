# tool: scribe-data
# All Latvian (Q9078) proper_nouns (Q147276) and their forms.
# Enter this query at https://query.wikidata.org/.

SELECT
  (REPLACE(STR(?lexeme), "http://www.wikidata.org/entity/", "") AS ?lexemeID)
  ?proper_nouns
  ?dativeSingular
  ?pluralGenitive
  ?vocativePlural
  ?locativeSingular
  ?instrumentalPlural
  ?genitiveSingular
  ?accusativeSingular
  ?pluralAccusative
  ?locativePlural
  ?nominativeSingular
  ?pluralDative
  ?instrumentalSingular
  ?pluralNominative
  ?vocativeSingular
  ?singularDative
  ?singularNominative
  ?accusativePlural
  ?nominativePlural
  ?singularInstrumental
  ?pluralInstrumental
  ?singularLocative
  ?pluralLocative
  ?singularGenitive
  ?dativePlural
  ?singularAccusative
  ?genitivePlural
  ?singularVocative
  ?pluralVocative
   ?lastModified

WHERE {
  ?lexeme dct:language wd:Q9078 ;
      wikibase:lexicalCategory wd:Q147276 ;
      wikibase:lemma ?proper_nouns ;
      schema:dateModified ?lastModified .
    
  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?dativeSingularForm .
    ?dativeSingularForm ontolex:representation ?dativeSingular ;
      wikibase:grammaticalFeature wd:Q145599, wd:Q110786 .
  }

  OPTIONAL {
