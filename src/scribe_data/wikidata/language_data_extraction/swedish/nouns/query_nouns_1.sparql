# tool: scribe-data
# All swedish (Q9027) nouns (Q1084) and their forms.
# Enter this query at https://query.wikidata.org/.

SELECT
   (REPLACE(STR(?lexeme), "http://www.wikidata.org/entity/", "") AS ?lexemeID)
    ?nouns
    ?genitiveIndefinite
  ?singularGenitiveDefinite
  ?genitivePluralDefinite
  ?genitive
  ?definite
  ?nominativePluralIndefinite
  ?singularNominativeDefinite
  ?nominativeIndefinite
  ?singularNominative
  ?oblique
  ?singularCommonPositiveIndefinite
  ?genitivePluralIndefinite
  ?singularGenitiveIndefinite
  ?genitiveDefinite
  ?singularNominativeIndefinite
  ?nominativePluralDefinite
  ?nominativeDefinite
  ?nominative
  ?genitiveDefiniteSingular
  ?genitiveIndefinitePlural
  ?nominativeDefinitePlural
  ?genitiveIndefiniteSingular
  ?nominativeIndefinitePlural
  ?nominativeIndefiniteSingular
  ?genitiveDefinitePlural
  ?nominativeDefiniteSingular
  WHERE {
    ?lexeme dct:language wd:Q9027 ;
        wikibase:lexicalCategory wd:Q1084 ;
        wikibase:lemma ?nouns .
        FILTER(lang(?nouns) = "sv")
    
        OPTIONAL {
            ?lexeme ontolex:lexicalForm ?genitiveIndefiniteForm .
            ?genitiveIndefiniteForm ontolex:representation ?genitiveIndefinite ;
                wikibase:grammaticalFeature wd:Q146233, wd:Q53997857 .
        }


        OPTIONAL {
            ?lexeme ontolex:lexicalForm ?singularGenitiveDefiniteForm .
            ?singularGenitiveDefiniteForm ontolex:representation ?singularGenitiveDefinite ;
                wikibase:grammaticalFeature wd:Q110786, wd:Q146233, wd:Q53997851 .
        }


        OPTIONAL {
            ?lexeme ontolex:lexicalForm ?genitivePluralDefiniteForm .
            ?genitivePluralDefiniteForm ontolex:representation ?genitivePluralDefinite ;
                wikibase:grammaticalFeature wd:Q146233, wd:Q146786, wd:Q53997851 .
        }


        OPTIONAL {
            ?lexeme ontolex:lexicalForm ?genitiveForm .
            ?genitiveForm ontolex:representation ?genitive ;
                wikibase:grammaticalFeature wd:Q146233 .
        }


        OPTIONAL {
            ?lexeme ontolex:lexicalForm ?definiteForm .
            ?definiteForm ontolex:representation ?definite ;
                wikibase:grammaticalFeature wd:Q53997851 .
        }


        OPTIONAL {
            ?lexeme ontolex:lexicalForm ?nominativePluralIndefiniteForm .
            ?nominativePluralIndefiniteForm ontolex:representation ?nominativePluralIndefinite ;
                wikibase:grammaticalFeature wd:Q131105, wd:Q146786, wd:Q53997857 .
        }


