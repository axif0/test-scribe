
# tool: scribe-data
# All czech (Q9056) nouns (Q1084) and their forms.
# Enter this query at https://query.wikidata.org/.

SELECT
   (REPLACE(STR(?lexeme), "http://www.wikidata.org/entity/", "") AS ?lexemeID)
    ?nouns
    ?singularNominative
  ?singularAccusativeNeuter
  ?nominativePluralNeuter
  ?singularDative
  ?vocative
  ?singularNeuterLocative
  ?singularGenitive
  ?pluralLocative
  ?singularLocative
  ?singularDativeNeuter
  ?singularNeuterInstrumental
  ?genitivePlural
  ?accusativePlural
  ?pluralInstrumental
  ?singularNeuterVocative
  ?singularInstrumental
  ?accusative
  ?dativePlural
  ?pluralVocative
  ?singularVocative
  ?nominative
  ?dative
  ?genitive
  ?locative
  ?singularAccusative
  ?instrumental
  WHERE {
    ?lexeme dct:language wd:Q9056 ;
        wikibase:lexicalCategory wd:Q1084 ;
        wikibase:lemma ?nouns .
        FILTER(lang(?nouns) = "cs")
    
        OPTIONAL {
            ?lexeme ontolex:lexicalForm ?singularNominativeForm .
            ?singularNominativeForm ontolex:representation ?singularNominative ;
                wikibase:grammaticalFeature wd:Q110786, wd:Q131105 .
        }
        OPTIONAL {
            ?lexeme ontolex:lexicalForm ?singularAccusativeNeuterForm .
            ?singularAccusativeNeuterForm ontolex:representation ?singularAccusativeNeuter ;
                wikibase:grammaticalFeature wd:Q110786, wd:Q146078, wd:Q1775461 .
        }
