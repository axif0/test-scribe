# tool: scribe-data
# All Estonian (Q9072) personal_pronouns (Q468801) and their forms.
# Enter this query at https://query.wikidata.org/.

SELECT
  (REPLACE(STR(?lexeme), "http://www.wikidata.org/entity/", "") AS ?lexemeID)
  ?personal_pronouns
  ?nominative
  ?singularTranslative
  ?singularIllative
  ?partitive
  ?essive
  ?singularNominative
  ?singularPartitive
  ?terminative
  ?inessive
  ?allative
  ?singularInessive
  ?singularTerminative
  ?singularEssive
  ?comitative
  ?singularAllative
  ?abessive
  ?genitive
  ?singularComitative
  ?ablative
  ?singularAbessive
  ?adessive
  ?singularAblative
  ?elative
  ?singularGenitive
  ?singularAdessive
  ?singularElative
  ?translative
  ?illative
 ?lastModified

WHERE {
  ?lexeme dct:language wd:Q9072 ;
      wikibase:lexicalCategory wd:Q468801 ;
      wikibase:lemma ?personal_pronouns .
      schema:dateModified ?lastModified .
    
  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?nominativeForm .
    ?nominativeForm ontolex:representation ?nominative ;
      wikibase:grammaticalFeature wd:Q131105 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?singularTranslativeForm .
    ?singularTranslativeForm ontolex:representation ?singularTranslative ;
      wikibase:grammaticalFeature wd:Q110786, wd:Q950170 .
  }

