# tool: scribe-data
# All Estonian (Q9072) postpositions (Q161873) and their forms.
# Enter this query at https://query.wikidata.org/.

SELECT
  (REPLACE(STR(?lexeme), "http://www.wikidata.org/entity/", "") AS ?lexemeID)
  ?postpositions
  ?singularTranslative
  ?singularIllative
  ?pluralTranslative
  ?singularNominative
  ?singularPartitive
  ?pluralIllative
  ?singularInessive
  ?pluralPartitive
  ?singularEssive
  ?singularTerminative
  ?singularAllative
  ?pluralEssive
  ?nominativePlural
  ?pluralInessive
  ?singularComitative
  ?pluralTerminative
  ?singularAbessive
  ?pluralAllative
  ?genitivePlural
  ?pluralComitative
  ?singularAblative
  ?singularGenitive
  ?pluralAbessive
  ?singularAdessive
  ?singularElative
  ?pluralAblative
  ?pluralAdessive
  ?pluralElative
 ?lastModified

WHERE {
  ?lexeme dct:language wd:Q9072 ;
      wikibase:lexicalCategory wd:Q161873 ;
      wikibase:lemma ?postpositions .
      schema:dateModified ?lastModified .
    
  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?singularTranslativeForm .
    ?singularTranslativeForm ontolex:representation ?singularTranslative ;
      wikibase:grammaticalFeature wd:Q110786, wd:Q950170 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?singularIllativeForm .
    ?singularIllativeForm ontolex:representation ?singularIllative ;
      wikibase:grammaticalFeature wd:Q110786, wd:Q474668 .
  }

