# tool: scribe-data
# All Norwegian (Q25167) nouns (Q1084) and their forms.
# Enter this query at https://query.wikidata.org/.

SELECT
  (REPLACE(STR(?lexeme), "http://www.wikidata.org/entity/", "") AS ?lexemeID)
  ?nouns
  ?singularMasculineIndefinite
  ?pluralNeuterMasculineDefinite
  ?pluralFeminineNeuterDefinite
  ?pluralDefinite
  ?pluralMasculineIndefinite
  ?singularNeuterDefinite
  ?pluralNeuterDefinite
  ?pluralFeminineMasculineIndefinite
  ?pluralFeminineNeuterMasculineDefinite
  ?singularFeminineDefinite
  ?pluralIndefinite
  ?pluralNeuterMasculineIndefinite
  ?pluralFeminineNeuterIndefinite
  ?pluralFeminineDefinite
  ?neuter
  ?plural
  ?singularDefinite
  ?pluralNeuterIndefinite
  ?singularMasculineDefinite
  ?singular
  ?pluralFeminineNeuterMasculineIndefinite
  ?pluralFeminineIndefinite
  ?pluralMasculineDefinite
  ?singularIndefinite
  ?indefinitePlural
  ?definitePlural
  ?definiteSingular
   ?lastModified

WHERE {
  ?lexeme dct:language wd:Q25167 ;
      wikibase:lexicalCategory wd:Q1084 ;
      wikibase:lemma ?nouns ;
      schema:dateModified ?lastModified .
    
  # Note: We need to filter for nb to remove bokm√•l (nb) words.
  FILTER(lang(?nouns) = "nb")
    
  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?singularMasculineIndefiniteForm .
    ?singularMasculineIndefiniteForm ontolex:representation ?singularMasculineIndefinite ;
      wikibase:grammaticalFeature wd:Q110786, wd:Q499327, wd:Q53997857 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?pluralNeuterMasculineDefiniteForm .
    ?pluralNeuterMasculineDefiniteForm ontolex:representation ?pluralNeuterMasculineDefinite ;
      wikibase:grammaticalFeature wd:Q146786, wd:Q1775461, wd:Q499327, wd:Q53997851 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?pluralFeminineNeuterDefiniteForm .
    ?pluralFeminineNeuterDefiniteForm ontolex:representation ?pluralFeminineNeuterDefinite ;
      wikibase:grammaticalFeature wd:Q146786, wd:Q1775415, wd:Q1775461, wd:Q53997851 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?pluralDefiniteForm .
    ?pluralDefiniteForm ontolex:representation ?pluralDefinite ;
      wikibase:grammaticalFeature wd:Q146786, wd:Q53997851 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?pluralMasculineIndefiniteForm .
    ?pluralMasculineIndefiniteForm ontolex:representation ?pluralMasculineIndefinite ;
      wikibase:grammaticalFeature wd:Q146786, wd:Q499327, wd:Q53997857 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?singularNeuterDefiniteForm .
    ?singularNeuterDefiniteForm ontolex:representation ?singularNeuterDefinite ;
      wikibase:grammaticalFeature wd:Q110786, wd:Q1775461, wd:Q53997851 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?pluralNeuterDefiniteForm .
    ?pluralNeuterDefiniteForm ontolex:representation ?pluralNeuterDefinite ;
      wikibase:grammaticalFeature wd:Q146786, wd:Q1775461, wd:Q53997851 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?pluralFeminineMasculineIndefiniteForm .
    ?pluralFeminineMasculineIndefiniteForm ontolex:representation ?pluralFeminineMasculineIndefinite ;
      wikibase:grammaticalFeature wd:Q146786, wd:Q1775415, wd:Q499327, wd:Q53997857 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?pluralFeminineNeuterMasculineDefiniteForm .
    ?pluralFeminineNeuterMasculineDefiniteForm ontolex:representation ?pluralFeminineNeuterMasculineDefinite ;
      wikibase:grammaticalFeature wd:Q146786, wd:Q1775415, wd:Q1775461, wd:Q499327, wd:Q53997851 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?singularFeminineDefiniteForm .
    ?singularFeminineDefiniteForm ontolex:representation ?singularFeminineDefinite ;
      wikibase:grammaticalFeature wd:Q110786, wd:Q1775415, wd:Q53997851 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?pluralIndefiniteForm .
    ?pluralIndefiniteForm ontolex:representation ?pluralIndefinite ;
      wikibase:grammaticalFeature wd:Q146786, wd:Q53997857 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?pluralNeuterMasculineIndefiniteForm .
    ?pluralNeuterMasculineIndefiniteForm ontolex:representation ?pluralNeuterMasculineIndefinite ;
      wikibase:grammaticalFeature wd:Q146786, wd:Q1775461, wd:Q499327, wd:Q53997857 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?pluralFeminineNeuterIndefiniteForm .
    ?pluralFeminineNeuterIndefiniteForm ontolex:representation ?pluralFeminineNeuterIndefinite ;
      wikibase:grammaticalFeature wd:Q146786, wd:Q1775415, wd:Q1775461, wd:Q53997857 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?pluralFeminineDefiniteForm .
    ?pluralFeminineDefiniteForm ontolex:representation ?pluralFeminineDefinite ;
      wikibase:grammaticalFeature wd:Q146786, wd:Q1775415, wd:Q53997851 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?neuterForm .
    ?neuterForm ontolex:representation ?neuter ;
      wikibase:grammaticalFeature wd:Q1775461 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?pluralForm .
    ?pluralForm ontolex:representation ?plural ;
      wikibase:grammaticalFeature wd:Q146786 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?singularDefiniteForm .
    ?singularDefiniteForm ontolex:representation ?singularDefinite ;
      wikibase:grammaticalFeature wd:Q110786, wd:Q53997851 .
