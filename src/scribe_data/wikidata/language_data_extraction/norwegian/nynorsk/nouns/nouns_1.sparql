    ?pluralNeuterIndefiniteForm ontolex:representation ?pluralNeuterIndefinite ;
      wikibase:grammaticalFeature wd:Q146786, wd:Q1775461, wd:Q53997857 .
  }

  OPTIONAL {
    ?lexeme ontolex:lexicalForm ?pluralIndefiniteForm .
